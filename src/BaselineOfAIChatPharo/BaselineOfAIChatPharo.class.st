Class {
	#name : 'BaselineOfAIChatPharo',
	#superclass : 'BaselineOf',
	#category : 'BaselineOfAIChatPharo',
	#package : 'BaselineOfAIChatPharo'
}

{ #category : 'baselines' }
BaselineOfAIChatPharo class >> base64ToMorph: aString [
    
    ^ (PNGReadWriter on:
           (ZnBase64Encoder new decode: aString) readStream)
          nextImage asMorph
]

{ #category : 'baselines' }
BaselineOfAIChatPharo class >> chatPharoLogo [

	<script: 'self chatPharoLogo form asPresenter open'>
	^ self base64ToMorph: self chatPharoLogoBase64
]

{ #category : 'baselines' }
BaselineOfAIChatPharo class >> chatPharoLogoBase64 [

	^ ''
]

{ #category : 'baselines' }
BaselineOfAIChatPharo class >> chatPharoLogoDark [

	<script: 'self chatPharoLogoDark form asPresenter open'>
	^ self base64ToMorph: self chatPharoLogoDarkBase64
]

{ #category : 'baselines' }
BaselineOfAIChatPharo class >> chatPharoLogoDarkBase64 [ 
	
	^ ''
]

{ #category : 'baselines' }
BaselineOfAIChatPharo class >> loadBackgroundImage [

    "Load Cormas logo as background image"
    <script>
    | logo |
    
    logo := (Smalltalk ui theme isKindOf: PharoLightTheme)
        ifTrue: [ self chatPharoLogo ]
        ifFalse: [ self chatPharoLogoDark ].
        
    PolymorphSystemSettings setDesktopLogoWith: logo
]

{ #category : 'baselines' }
BaselineOfAIChatPharo >> baseline: spec [

	<baseline>
	spec for: #common do: [
			spec postLoadDoIt: #postLoad.
			spec package: 'AI-ChatPharo'.
			spec
				package: 'AI-ChatPharo-Agent'
				with: [ spec requires: #( 'AI-ChatPharo' ) ].
			spec
				package: 'AI-ChatPharo-Spec'
				with: [ spec requires: #( 'AI-ChatPharo' ) ].
			spec
				package: 'AI-ChatPharo-Tools'
				with: [ spec requires: #( 'AI-ChatPharo' ) ].
			spec
				package: 'AI-ChatPharo-History'
				with: [ spec requires: #( 'AI-ChatPharo' ) ].
			spec
				package: 'AI-ChatPharo-Env'
				with: [ spec requires: #( 'AI-ChatPharo' ) ].
			spec
				package: 'AI-ChatPharo-Safety'
				with: [ spec requires: #( 'AI-ChatPharo' ) ].
			spec package: 'AI-ChatPharo-Tests' with: [
					spec requires:
						#( 'AI-ChatPharo' 'AI-ChatPharo-Agent' 'AI-ChatPharo-Spec'
						   'AI-ChatPharo-Tools' 'AI-ChatPharo-History'
						   'AI-ChatPharo-Env' ) ] ]
]

{ #category : 'baselines' }
BaselineOfAIChatPharo >> postLoad [
	"UIManager default inform:
		'ChatPharo is currently in beta. You might encounter bugs or memory issues that could halt your image.'"

	self class loadBackgroundImage.
	"ChatPharoShortcuts installAsGlobalCategory."
	KMRepository reset.
	SystemAnnouncer uniqueInstance
		when: UIThemeChanged
		send: #loadBackgroundImage
		to: self class
]
